<keymap version="1" name="Default mySelf Proper Redo" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="emacsIDEAs.AceJumpWord">
    <keyboard-shortcut first-keystroke="meta l" />
    <keyboard-shortcut first-keystroke="meta i" second-keystroke="meta f" />
    <keyboard-shortcut first-keystroke="alt k" />
  </action>
</keymap>