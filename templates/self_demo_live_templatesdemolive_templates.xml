<templateSet group="self_demo_live_templates（demo演示整理live_templates）">
  <template name="lt_demo_sout" value="System.out.println(&quot;----------$VAR1$值=&quot; + $VAR1$ + &quot;,&quot; + &quot;当前类=$CLASS_NAME$.$METHOD_NAME$()&quot;);$END$" toReformat="false" toShortenFQNames="true">
    <variable name="VAR1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CLASS_NAME" expression="className()" defaultValue="" alwaysStopAt="true" />
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="lt_demo_log" value="logger.info(&quot;$VAR1$:{} $CLASS_NAME$.$METHOD_NAME$() &quot;,$VAR1$);$END$" toReformat="false" toShortenFQNames="true">
    <variable name="VAR1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CLASS_NAME" expression="className()" defaultValue="" alwaysStopAt="true" />
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>