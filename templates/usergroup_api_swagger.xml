<templateSet group="usergroup_api_swagger">
  <template name="lt_api_model_property" value="@ApiModelProperty(value = &quot;$END$&quot;,required = true)" description="@ApiModelProperty-字段增加swagger" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="lt_api_model" value="@ApiModel&#10;@lombok.Data$END$" description="@ApiModel-实体类增加Swagger标识" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="lt_api" value="@Api(tags = &quot;$END$&quot;)&#10;@RestController&#10;@RequestMapping(&quot;&quot;)" description="@Api-标识，增加位置是Controller" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="lt_api_operation" value="@ApiOperation(value = &quot;$END$&quot;,notes = &quot;&quot;)&#10;@RequestMapping(value = &quot;&quot;, method = RequestMethod.POST)" description="@ApiOperation-具体的接口增加swagger-apioperation支持" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>