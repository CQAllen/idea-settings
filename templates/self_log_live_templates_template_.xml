<templateSet group="self_log_live_templates(日志template)">
  <template name="lt_log_util" value="// Logger打印日志对象&#10;$END$private static final Logger LOGGER = Logger.getLogger($CURRENT_CLASS$.class.getName());" description="logger日志对象" toReformat="false" toShortenFQNames="true">
    <variable name="CURRENT_CLASS" expression="className()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="lt_log_slf4j" value="// Logger打印日志对象&#10;$END$private static org.slf4j.Logger logger = org.slf4j.LoggerFactory.getLogger($CURRENT_CLASS$.class.getName());" description="logger日志对象" toReformat="false" toShortenFQNames="true">
    <variable name="CURRENT_CLASS" expression="className()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="lt_log_ex" value="logger.error(&quot;&quot;, ex);" description="增加异常日志" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>